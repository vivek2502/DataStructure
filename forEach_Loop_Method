import java.util.*;

public class Demo  {
  
  public static void main(String args[]){
    
  int a[]={1,2,3,4,5};
  List<Integer> list=Arrays.asList(1,2,3,4,5);
  //for each loop
  for(Integer l : list)
    System.out.println(l);
  //for each method
  list.forEach((n)-> System.out.println(n));
  //for loop
  for(int i=0;i<list.size();i++)
    System.out.println(list.get(i));
  }
  
}